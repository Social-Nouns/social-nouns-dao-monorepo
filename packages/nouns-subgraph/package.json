{
  "name": "@nouns/subgraph",
  "version": "0.1.0",
  "description": "A subgraph that indexes nouns events",
  "author": "Nounders",
  "homepage": "https://nouns.wtf",
  "license": "GPL-3.0",
  "main": "build/index.js",
  "directories": {
    "src": "src"
  },
  "scripts": {
    "clean": "rimraf ./src/types ./build subgraph.yaml",
    "codegen": "graph codegen --output-dir src/types/",
    "deploy": "graph deploy --debug --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ --access-token f2a8ae9f4c3a59603c1b3de62aff682e",
    "prepare:rinkeby": "mustache config/rinkeby.json subgraph.yaml.mustache > subgraph.yaml",
    "prepare:mainnet": "mustache config/mainnet.json subgraph.yaml.mustache > subgraph.yaml",
    "prepare:goerli": "mustache config/goerli-fork.json subgraph.yaml.mustache > subgraph.yaml",
    "deploy:rinkeby": "yarn clean && yarn prepare:rinkeby && yarn codegen && yarn deploy ",
    "deploy:goerli": "yarn clean && yarn prepare:goerli && yarn codegen && yarn deploy ",
    "deploy:mainnet": "yarn clean && yarn prepare:mainnet && yarn codegen && yarn deploy socialnouns/social-nouns",
    "mustache": "mustache"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.20.1",
    "@graphprotocol/graph-ts": "^0.20.0",
    "mustache": "^4.2.0"
  }
}
